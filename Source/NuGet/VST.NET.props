          <HintPath>$(MSBuildThisFileDirectory)..\VstNet\x86\Jacobi.Vst.Framework.dll</HintPath>
          <SpecificVersion>False</SpecificVersion>
		  <NugetPackageId>VST.NET</NugetPackageId>
        </Reference>
		<Reference Include="Jacobi.Vst.Interop, Version=1.1.3.0, Culture=neutral, PublicKeyToken=6a02705897431c03, processorArchitecture=x86">
          <HintPath>$(MSBuildThisFileDirectory)..\VstNet\x86\Jacobi.Vst.Interop.dll</HintPath>
          <SpecificVersion>False</SpecificVersion>
		  <NugetPackageId>VST.NET</NugetPackageId>
        </Reference>
      </ItemGroup>
    </Otherwise>
  </Choose>
</Project>